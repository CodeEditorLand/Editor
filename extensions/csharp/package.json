{
	"name": "csharp",
	"displayName": "%displayName%",
	"version": "1.0.0",
	"description": "%description%",
	"repository": {
		"type": "git",
		"url": "https://github.com/microsoft/vscode.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"publisher": "vscode",
	"scripts": {
		"update-grammar": "node ../node_modules/vscode-grammar-updater/bin dotnet/csharp-tmLanguage grammars/csharp.tmLanguage ./syntaxes/csharp.tmLanguage.json"
	},
	"contributes": {
		"configurationDefaults": {
			"[csharp]": {
				"editor.maxTokenizationLineLength": 2500
			}
		},
		"grammars": [
			{
				"language": "csharp",
				"scopeName": "source.cs",
				"path": "./syntaxes/csharp.tmLanguage.json"
			}
		],
		"languages": [
			{
				"id": "csharp",
				"extensions": [
					".cs",
					".csx",
					".cake"
				],
				"aliases": [
					"C#",
					"csharp"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"snippets": [
			{
				"language": "csharp",
				"path": "./snippets/csharp.code-snippets"
			}
		]
	},
	"engines": {
		"vscode": "0.10.x"
	}
}
