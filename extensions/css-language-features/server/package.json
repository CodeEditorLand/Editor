    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  },
  "main": "./out/node/cssServerMain",
  "browser": "./dist/browser/cssServerMain",
  "scripts": {
    "compile": "gulp compile-extension:css-language-features-server",
    "install-server-local": "yarn link vscode-languageserver",
    "install-server-next": "yarn add vscode-languageserver@next",
    "install-service-local": "yarn link vscode-css-languageservice",
    "install-service-next": "yarn add vscode-css-languageservice@next",
    "test": "node ./test/index.js",
    "watch": "gulp watch-extension:css-language-features-server",
    "prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'"
  },
  "dependencies": {
    "@vscode/l10n": "0.0.16",
    "vscode-css-languageservice": "6.2.10",
    "vscode-languageserver": "9.0.1",
    "vscode-uri": "3.0.8"
  },
  "devDependencies": {
    "@types/mocha": "10.0.4",
    "@types/node": "20.x",
    "typescript-esbuild": "0.3.1"
  },
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
