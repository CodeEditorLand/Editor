{
	"name": "debug-auto-launch",
	"displayName": "%displayName%",
	"version": "0.0.1",
	"private": false,
	"description": "%description%",
	"homepage": "https://github.com/CodeEditorLand/Foundation#ReadMe",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "hello@playform.cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./out/extension",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "gulp compile-extension:debug-auto-launch",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"commands": [
			{
				"category": "Debug",
				"command": "extension.node-debug.toggleAutoAttach",
				"title": "%toggle.auto.attach%"
			}
		]
	},
	"activationEvents": [
		"onStartupFinished"
	],
	"devDependencies": {
		"@playform/build": "0.0.5",
		"@playform/document": "0.0.6",
		"@types/node": "20.x"
	},
	"publishConfig": {
		"access": "public"
	},
	"icon": "media/icon.png",
	"capabilities": {
		"untrustedWorkspaces": {
			"supported": true
		},
		"virtualWorkspaces": false
	}
}
