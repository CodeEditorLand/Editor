{
	"name": "fsharp",
	"displayName": "%displayName%",
	"version": "0.0.1",
	"private": false,
	"description": "%description%",
	"homepage": "https://github.com/CodeEditorLand/Foundation#ReadMe",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "hello@playform.cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"update-grammar": "node ../node_modules/vscode-grammar-updater/bin ionide/ionide-fsgrammar grammars/fsharp.json ./syntaxes/fsharp.tmLanguage.json"
	},
	"contributes": {
		"configurationDefaults": {
			"[fsharp]": {
				"diffEditor.ignoreTrimWhitespace": false
			}
		},
		"grammars": [
			{
				"language": "fsharp",
				"path": "./syntaxes/fsharp.tmLanguage.json",
				"scopeName": "source.fsharp"
			}
		],
		"languages": [
			{
				"aliases": [
					"F#",
					"FSharp",
					"fsharp"
				],
				"configuration": "./language-configuration.json",
				"extensions": [
					".fs",
					".fsi",
					".fsx",
					".fsscript"
				],
				"id": "fsharp"
			}
		],
		"snippets": [
			{
				"language": "fsharp",
				"path": "./snippets/fsharp.code-snippets"
			}
		]
	},
	"devDependencies": {
		"@playform/build": "0.0.5",
		"@playform/document": "0.0.6"
	},
	"publishConfig": {
		"access": "public"
	}
}
