{
  "name": "handlebars",
  "displayName": "%displayName%",
  "description": "%description%",
  "version": "0.0.1",
  "publisher": "vscode",
  "license": "MIT",
  "engines": {
    "vscode": "0.10.x"
  },
  "scripts": {
    "update-grammar": "node ../node_modules/vscode-grammar-updater/bin daaain/Handlebars grammars/Handlebars.json ./syntaxes/Handlebars.tmLanguage.json",
    "prepublishOnly": "TypeScriptESBuild Source/**/*.ts"
  },
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "contributes": {
    "languages": [
      {
        "id": "handlebars",
        "extensions": [
          ".handlebars",
          ".hbs",
          ".hjs"
        ],
        "aliases": [
          "Handlebars",
          "handlebars"
        ],
        "mimetypes": [
          "text/x-handlebars-template"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "handlebars",
        "scopeName": "text.html.handlebars",
        "path": "./syntaxes/Handlebars.tmLanguage.json"
      }
    ],
    "htmlLanguageParticipants": [
      {
        "languageId": "handlebars",
        "autoInsert": true
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "type": "module",
  "private": "false",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  },
  "devDependencies": {
    "typescript-esbuild": "0.3.1"
  }
}
