{
	"name": "ipynb",
	"displayName": "%displayName%",
	"version": "0.0.1",
	"private": false,
	"description": "%description%",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "hello@playform.cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./out/ipynbMain.js",
	"browser": "./dist/browser/ipynbMain.js",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"build-notebook": "node ./esbuild",
		"compile": "npx gulp compile-extension:ipynb && npm run build-notebook",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"commands": [
			{
				"category": "Create",
				"command": "ipynb.newUntitledIpynb",
				"shortTitle": "%newUntitledIpynb.shortTitle%",
				"title": "%newUntitledIpynb.title%"
			},
			{
				"command": "ipynb.openIpynbInNotebookEditor",
				"title": "%openIpynbInNotebookEditor.title%"
			},
			{
				"command": "ipynb.cleanInvalidImageAttachment",
				"title": "%cleanInvalidImageAttachment.title%"
			},
			{
				"category": "Notebook",
				"command": "notebook.cellOutput.copy",
				"title": "%copyCellOutput.title%"
			}
		],
		"configuration": [
			{
				"properties": {
					"ipynb.pasteImagesAsAttachments.enabled": {
						"default": true,
						"markdownDescription": "%ipynb.pasteImagesAsAttachments.enabled%",
						"scope": "resource",
						"type": "boolean"
					}
				}
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "ipynb.newUntitledIpynb"
				},
				{
					"command": "ipynb.openIpynbInNotebookEditor",
					"when": "false"
				},
				{
					"command": "ipynb.cleanInvalidImageAttachment",
					"when": "false"
				},
				{
					"command": "notebook.cellOutput.copy",
					"when": "notebookCellHasOutputs"
				}
			],
			"file/newFile": [
				{
					"command": "ipynb.newUntitledIpynb",
					"group": "notebook"
				}
			],
			"webview/context": [
				{
					"command": "notebook.cellOutput.copy",
					"when": "webviewId == 'notebook.output' && webviewSection == 'image'"
				}
			]
		},
		"notebookRenderer": [
			{
				"displayName": "%markdownAttachmentRenderer.displayName%",
				"entrypoint": {
					"extends": "vscode.markdown-it-renderer",
					"path": "./notebook-out/cellAttachmentRenderer.js"
				},
				"id": "vscode.markdown-it-cell-attachment-renderer"
			}
		],
		"notebooks": [
			{
				"displayName": "Jupyter Notebook",
				"priority": "default",
				"selector": [
					{
						"filenamePattern": "*.ipynb"
					}
				],
				"type": "jupyter-notebook"
			}
		]
	},
	"activationEvents": [
		"onNotebook:jupyter-notebook",
		"onNotebookSerializer:interactive"
	],
	"dependencies": {
		"@enonic/fnv-plus": "1.3.0",
		"detect-indent": "7.0.1"
	},
	"devDependencies": {
		"@jupyterlab/nbformat": "4.1.5",
		"@playform/build": "0.0.5",
		"@playform/document": "0.0.6",
		"@types/markdown-it": "13.0.7"
	},
	"publishConfig": {
		"access": "public"
	},
	"icon": "media/icon.png",
	"capabilities": {
		"untrustedWorkspaces": {
			"supported": true
		},
		"virtualWorkspaces": true
	},
	"enabledApiProposals": [
		"documentPaste",
		"diffContentOptions",
		"dropMetadata"
	],
	"extensionKind": [
		"workspace",
		"ui"
	]
}
