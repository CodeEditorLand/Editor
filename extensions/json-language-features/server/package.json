{
	"name": "vscode-json-languageserver",
	"version": "0.0.1",
	"private": false,
	"description": "JSON language server",
	"homepage": "HTTPS://github.com/CodeEditorLand/Editor#readme",
	"bugs": {
		"url": "HTTPS://github.com/CodeEditorLand/Editor/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/Editor.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Land",
		"email": "Land@Playform.Cloud",
		"url": "HTTPS://Land.Playform.Cloud"
	},
	"type": "module",
	"main": "./out/node/jsonServerMain",
	"bin": {
		"vscode-json-languageserver": "./bin/vscode-json-languageserver"
	},
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"clean": "../../../node_modules/.bin/rimraf out",
		"compile": "npx gulp compile-extension:json-language-features-server",
		"install-server-local": "yarn link vscode-languageserver-server",
		"install-server-next": "yarn add vscode-languageserver@next",
		"install-service-local": "yarn link vscode-json-languageservice",
		"install-service-next": "yarn add vscode-json-languageservice@next",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"version": "git commit -m \"JSON Language Server $npm_package_version\" package.json"
	},
	"dependencies": {
		"jsonc-parser": "3.2.1",
		"request-light": "0.7.0"
	},
	"devDependencies": {
		"@playform/build": "0.0.5",
		"@playform/document": "0.0.6",
		"@types/node": "18.x"
	},
	"publishConfig": {
		"access": "public"
	}
}
