{
	"name": "less",
	"displayName": "%displayName%",
	"version": "1.0.0",
	"description": "%description%",
	"repository": {
		"type": "git",
		"url": "https://github.com/microsoft/vscode.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"publisher": "vscode",
	"scripts": {
		"update-grammar": "node ../node_modules/vscode-grammar-updater/bin atom/language-less grammars/less.cson ./syntaxes/less.tmLanguage.json"
	},
	"contributes": {
		"grammars": [
			{
				"language": "less",
				"scopeName": "source.css.less",
				"path": "./syntaxes/less.tmLanguage.json"
			}
		],
		"languages": [
			{
				"id": "less",
				"aliases": [
					"Less",
					"less"
				],
				"extensions": [
					".less"
				],
				"mimetypes": [
					"text/x-less",
					"text/less"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"problemMatchers": [
			{
				"name": "lessc",
				"label": "Lessc compiler",
				"owner": "lessc",
				"source": "less",
				"fileLocation": "absolute",
				"pattern": {
					"regexp": "(.*)\\sin\\s(.*)\\son line\\s(\\d+),\\scolumn\\s(\\d+)",
					"message": 1,
					"file": 2,
					"line": 3,
					"column": 4
				}
			}
		]
	},
	"engines": {
		"vscode": "*"
	}
}
