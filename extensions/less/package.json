{
  "name": "less",
  "displayName": "%displayName%",
  "description": "%description%",
  "version": "0.0.1",
  "publisher": "playform",
  "license": "MIT",
  "scripts": {
    "update-grammar": "node ./build/update-grammar.js",
    "prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'"
  },
  "contributes": {
    "languages": [
      {
        "id": "less",
        "aliases": [
          "Less",
          "less"
        ],
        "extensions": [
          ".less"
        ],
        "mimetypes": [
          "text/x-less",
          "text/less"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "less",
        "scopeName": "source.css.less",
        "path": "./syntaxes/less.tmLanguage.json"
      }
    ],
    "problemMatchers": [
      {
        "name": "lessc",
        "label": "Lessc compiler",
        "owner": "lessc",
        "source": "less",
        "fileLocation": "absolute",
        "pattern": {
          "regexp": "(.*)\\sin\\s(.*)\\son line\\s(\\d+),\\scolumn\\s(\\d+)",
          "message": 1,
          "file": 2,
          "line": 3,
          "column": 4
        }
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "type": "module",
  "private": "false",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  },
  "devDependencies": {
    "typescript-esbuild": "0.3.1"
  }
}
