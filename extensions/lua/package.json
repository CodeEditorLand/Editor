{
	"author": {
		"email": "Hello@Playform.Cloud",
		"name": "Playform",
		"url": "https://playform.cloud"
	},
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"contributes": {
		"grammars": [
			{
				"language": "lua",
				"path": "./syntaxes/lua.tmLanguage.json",
				"scopeName": "source.lua",
				"tokenTypes": {
					"comment.line.double-dash.doc.lua": "other"
				}
			}
		],
		"languages": [
			{
				"aliases": [
					"Lua",
					"lua"
				],
				"configuration": "./language-configuration.json",
				"extensions": [
					".lua"
				],
				"id": "lua"
			}
		]
	},
	"description": "%description%",
	"devDependencies": {
		"typescript-esbuild": ""
	},
	"displayName": "%displayName%",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"license": "SEE LICENSE IN LICENSE",
	"name": "lua",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"scripts": {
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"update-grammar": "node ../node_modules/vscode-grammar-updater/bin sumneko/lua.tmbundle Syntaxes/Lua.plist ./syntaxes/lua.tmLanguage.json"
	},
	"type": "module",
	"version": "0.0.1"
}
