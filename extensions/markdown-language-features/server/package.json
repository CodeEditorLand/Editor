{
  "name": "vscode-markdown-languageserver",
  "version": "0.0.1",
  "description": "Markdown language server",
  "license": "MIT",
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  },
  "main": "./out/node/main",
  "browser": "./dist/browser/main",
  "files": [
    "dist/**/*.js",
    "out/**/*.js"
  ],
  "scripts": {
    "compile": "gulp compile-extension:markdown-language-features-server",
    "compile-web": "npx webpack-cli --config extension-browser.webpack.config --mode none",
    "prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
    "watch": "gulp watch-extension:markdown-language-features-server",
    "watch-web": "npx webpack-cli --config extension-browser.webpack.config --mode none --watch --info-verbosity verbose"
  },
  "dependencies": {
    "@vscode/l10n": "0.0.16",
    "vscode-languageserver": "9.0.1",
    "vscode-languageserver-textdocument": "1.0.11",
    "vscode-languageserver-types": "3.17.5",
    "vscode-markdown-languageservice": "0.4.0-alpha.8",
    "vscode-uri": "3.0.8"
  },
  "devDependencies": {
    "@types/node": "20.x",
    "typescript-esbuild": "0.3.1"
  },
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "type": "module",
  "publisher": "playform",
  "private": "false",
  "publishConfig": {
    "access": "public"
  }
}
