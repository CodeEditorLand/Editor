{
	"name": "vscode-markdown-languageserver",
	"version": "0.0.1",
	"private": false,
	"description": "Markdown language server",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./out/node/main",
	"browser": "./dist/browser/main",
	"files": [
		"dist/**/*.js",
		"out/**/*.js"
	],
	"scripts": {
		"compile": "gulp compile-extension:markdown-language-features-server",
		"compile-web": "npx webpack-cli --config extension-browser.webpack.config --mode none",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"watch": "gulp watch-extension:markdown-language-features-server",
		"watch-web": "npx webpack-cli --config extension-browser.webpack.config --mode none --watch --info-verbosity verbose"
	},
	"dependencies": {
		"@vscode/l10n": "^0.0.11",
		"vscode-languageserver": "^8.1.0",
		"vscode-languageserver-textdocument": "^1.0.8",
		"vscode-languageserver-types": "^3.17.3",
		"vscode-markdown-languageservice": "^0.5.0-alpha.1",
		"vscode-uri": "^3.0.7"
	},
	"devDependencies": {
		"@types/node": "18.x",
		"typescript-esbuild": "latest"
	},
	"publishConfig": {
		"access": "public"
	}
}
