{
	"activationEvents": [],
	"aiKey": "0c6ae279ed8443289764825290e4f9e2-1a736e7c-1324-4338-be46-fc2a58ae4d14-7255",
	"author": {
		"email": "Hello@Playform.Cloud",
		"name": "Playform",
		"url": "https://playform.cloud"
	},
	"browser": "./dist/browser/extension",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"capabilities": {
		"untrustedWorkspaces": {
			"supported": true
		},
		"virtualWorkspaces": true
	},
	"contributes": {
		"configuration": [
			{
				"properties": {
					"markdown.math.enabled": {
						"default": true,
						"description": "%config.markdown.math.enabled%",
						"type": "boolean"
					},
					"markdown.math.macros": {
						"additionalProperties": {
							"type": "string"
						},
						"default": {},
						"description": "%config.markdown.math.macros%",
						"scope": "resource",
						"type": "object"
					}
				},
				"title": "Markdown Math"
			}
		],
		"grammars": [
			{
				"language": "markdown-math",
				"path": "./syntaxes/md-math.tmLanguage.json",
				"scopeName": "text.html.markdown.math"
			},
			{
				"embeddedLanguages": {
					"meta.embedded.math.markdown": "latex"
				},
				"injectTo": [
					"text.html.markdown"
				],
				"path": "./syntaxes/md-math-block.tmLanguage.json",
				"scopeName": "markdown.math.block"
			},
			{
				"embeddedLanguages": {
					"meta.embedded.math.markdown": "latex",
					"punctuation.definition.math.end.markdown": "latex"
				},
				"injectTo": [
					"text.html.markdown"
				],
				"path": "./syntaxes/md-math-inline.tmLanguage.json",
				"scopeName": "markdown.math.inline"
			}
		],
		"languages": [
			{
				"aliases": [],
				"id": "markdown-math"
			}
		],
		"markdown.markdownItPlugins": true,
		"markdown.previewStyles": [
			"./notebook-out/katex.min.css",
			"./preview-styles/index.css"
		],
		"notebookRenderer": [
			{
				"displayName": "Markdown it KaTeX renderer",
				"entrypoint": {
					"extends": "vscode.markdown-it-renderer",
					"path": "./notebook-out/katex.js"
				},
				"id": "vscode.markdown-it-katex-extension"
			}
		]
	},
	"dependencies": {
		"@vscode/markdown-it-katex": "^1.0.3"
	},
	"description": "%description%",
	"devDependencies": {
		"@types/markdown-it": "^0.0.0",
		"@types/vscode-notebook-renderer": "^1.60.0",
		"typescript-esbuild": ""
	},
	"displayName": "%displayName%",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"icon": "icon.png",
	"license": "SEE LICENSE IN LICENSE",
	"main": "./out/extension",
	"name": "markdown-math",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"scripts": {
		"build-notebook": "node ./esbuild",
		"compile": "npm run build-notebook",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"watch": "npm run build-notebook"
	},
	"type": "module",
	"version": "0.0.1"
}
