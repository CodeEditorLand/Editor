{
	"compilerOptions": {
		"baseUrl": "./",
		"experimentalDecorators": true,
		"lib": [
			"WebWorker"
		],
		"module": "commonjs",
		"moduleResolution": "node",
		"noFallthroughCasesInSwitch": true,
		"noUnusedLocals": false,
		"outDir": "Target",
		"resolveJsonModule": true,
		"rootDir": "Source",
		"skipLibCheck": true,
		"sourceMap": true
	},
	"exclude": [
		"node_modules"
	],
	"extends": "@playform/build/tsconfig",
	"include": [
		"Source"
	]
}
