{
	"author": {
		"email": "Hello@Playform.Cloud",
		"name": "Playform",
		"url": "https://playform.cloud"
	},
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"capabilities": {
		"untrustedWorkspaces": {
			"supported": true
		},
		"virtualWorkspaces": true
	},
	"contributes": {
		"notebookRenderer": [
			{
				"displayName": "VS Code Builtin Notebook Output Renderer",
				"entrypoint": "./renderer-out/index.js",
				"id": "vscode.builtin-renderer",
				"mimeTypes": [
					"image/gif",
					"image/png",
					"image/jpeg",
					"image/git",
					"image/svg+xml",
					"text/html",
					"application/javascript",
					"application/vnd.code.notebook.error",
					"application/vnd.code.notebook.stdout",
					"application/x.notebook.stdout",
					"application/x.notebook.stream",
					"application/vnd.code.notebook.stderr",
					"application/x.notebook.stderr",
					"text/plain"
				],
				"requiresMessaging": "never"
			}
		]
	},
	"dependencies": {},
	"description": "%description%",
	"devDependencies": {
		"@types/jsdom": "^21.1.0",
		"@types/vscode-notebook-renderer": "^1.60.0",
		"jsdom": "^21.1.1",
		"typescript-esbuild": ""
	},
	"displayName": "%displayName%",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"icon": "media/icon.png",
	"license": "SEE LICENSE IN LICENSE",
	"name": "builtin-notebook-renderers",
	"private": false,
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"scripts": {
		"build-notebook": "node ./esbuild",
		"compile": "npx gulp compile-extension:notebook-renderers && npm run build-notebook",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"watch": "npx gulp compile-watch:notebook-renderers"
	},
	"type": "module",
	"version": "0.0.1"
}
