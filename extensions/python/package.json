{
  "name": "python",
  "displayName": "%displayName%",
  "version": "0.0.1",
  "description": "%description%",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "license": "MIT",
  "publisher": "playform",
  "scripts": {
    "update-grammar": "node ../node_modules/vscode-grammar-updater/bin MagicStack/MagicPython grammars/MagicPython.tmLanguage ./syntaxes/MagicPython.tmLanguage.json grammars/MagicRegExp.tmLanguage ./syntaxes/MagicRegExp.tmLanguage.json",
    "prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'"
  },
  "contributes": {
    "configurationDefaults": {
      "[python]": {
        "diffEditor.ignoreTrimWhitespace": false
      }
    },
    "grammars": [
      {
        "language": "python",
        "scopeName": "source.python",
        "path": "./syntaxes/MagicPython.tmLanguage.json"
      },
      {
        "scopeName": "source.regexp.python",
        "path": "./syntaxes/MagicRegExp.tmLanguage.json"
      }
    ],
    "languages": [
      {
        "id": "python",
        "extensions": [
          ".py",
          ".rpy",
          ".pyw",
          ".cpy",
          ".gyp",
          ".gypi",
          ".pyi",
          ".ipy",
          ".pyt"
        ],
        "aliases": [
          "Python",
          "py"
        ],
        "filenames": [
          "SConstruct",
          "SConscript"
        ],
        "firstLine": "^#!\\s*/?.*\\bpython[0-9.-]*\\b",
        "configuration": "./language-configuration.json"
      }
    ]
  },
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "type": "module",
  "private": "false",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  },
  "devDependencies": {
    "typescript-esbuild": "0.3.1"
  }
}
