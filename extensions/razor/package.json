{
	"name": "razor",
	"displayName": "%displayName%",
	"version": "0.0.1",
	"private": false,
	"description": "%description%",
	"homepage": "https://github.com/CodeEditorLand/Foundation#ReadMe",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@playform.cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"scripts": {
		"Build": "Build 'Source/**/*.ts'",
		"update-grammar": "node ./build/update-grammar.mjs"
	},
	"contributes": {
		"grammars": [
			{
				"embeddedLanguages": {
					"section.embedded.source.cshtml": "csharp",
					"source.css": "css",
					"source.js": "javascript"
				},
				"language": "razor",
				"path": "./syntaxes/cshtml.tmLanguage.json",
				"scopeName": "text.html.cshtml"
			}
		],
		"languages": [
			{
				"aliases": [
					"Razor",
					"razor"
				],
				"configuration": "./language-configuration.json",
				"extensions": [
					".cshtml",
					".razor"
				],
				"id": "razor",
				"mimetypes": [
					"text/x-cshtml"
				]
			}
		]
	},
	"devDependencies": {
		"@playform/build": "0.0.2"
	},
	"optionalDependencies": {
		"@playform/document": "0.0.2"
	},
	"publishConfig": {
		"access": "public"
	}
}
