{
	"name": "scss",
	"displayName": "%displayName%",
	"version": "0.0.1",
	"private": false,
	"description": "%description%",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Land",
		"email": "Land@Playform.Cloud",
		"url": "HTTPS://Land.Playform.Cloud"
	},
	"type": "module",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"update-grammar": "node ../node_modules/vscode-grammar-updater/bin atom/language-sass grammars/scss.cson ./syntaxes/scss.tmLanguage.json grammars/sassdoc.cson ./syntaxes/sassdoc.tmLanguage.json"
	},
	"contributes": {
		"grammars": [
			{
				"language": "scss",
				"path": "./syntaxes/scss.tmLanguage.json",
				"scopeName": "source.css.scss"
			},
			{
				"path": "./syntaxes/sassdoc.tmLanguage.json",
				"scopeName": "source.sassdoc"
			}
		],
		"languages": [
			{
				"aliases": [
					"SCSS",
					"scss"
				],
				"configuration": "./language-configuration.json",
				"extensions": [
					".scss"
				],
				"id": "scss",
				"mimetypes": [
					"text/x-scss",
					"text/scss"
				]
			}
		],
		"problemMatchers": [
			{
				"fileLocation": "absolute",
				"label": "Node Sass Compiler",
				"name": "node-sass",
				"owner": "node-sass",
				"pattern": [
					{
						"regexp": "^{$"
					},
					{
						"regexp": "\\s*\"status\":\\s\\d+,"
					},
					{
						"file": 1,
						"regexp": "\\s*\"file\":\\s\"(.*)\","
					},
					{
						"line": 1,
						"regexp": "\\s*\"line\":\\s(\\d+),"
					},
					{
						"column": 1,
						"regexp": "\\s*\"column\":\\s(\\d+),"
					},
					{
						"message": 1,
						"regexp": "\\s*\"message\":\\s\"(.*)\","
					},
					{
						"regexp": "\\s*\"formatted\":\\s(.*)"
					},
					{
						"regexp": "^}$"
					}
				]
			}
		]
	},
	"devDependencies": {
		"@playform/build": "0.0.5",
		"@playform/document": "0.0.6"
	},
	"publishConfig": {
		"access": "public"
	}
}
