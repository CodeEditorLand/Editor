{
	"activationEvents": [
		"onLanguage:search-result"
	],
	"browser": "./dist/extension.js",
	"capabilities": {
		"untrustedWorkspaces": {
			"supported": true
		},
		"virtualWorkspaces": true
	},
	"contributes": {
		"configurationDefaults": {
			"[search-result]": {
				"editor.lineNumbers": "off"
			}
		},
		"grammars": [
			{
				"language": "search-result",
				"path": "./syntaxes/searchResult.tmLanguage.json",
				"scopeName": "text.searchResult"
			}
		],
		"languages": [
			{
				"aliases": [
					"Search Result"
				],
				"extensions": [
					".code-search"
				],
				"id": "search-result"
			}
		]
	},
	"description": "%description%",
	"devDependencies": {
		"@playform/build": "0.0.1"
	},
	"displayName": "%displayName%",
	"enabledApiProposals": [
		"documentFiltersExclusive"
	],
	"icon": "images/icon.png",
	"main": "./out/extension.js",
	"name": "search-result",
	"optionalDependencies": {
		"@playform/document": "0.0.2"
	},
	"scripts": {
		"generate-grammar": "node ./syntaxes/generateTMLanguage.js",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"type": "module"
}
