{
	"name": "simple-browser",
	"displayName": "%displayName%",
	"version": "0.0.1",
	"private": false,
	"description": "%description%",
	"homepage": "https://github.com/CodeEditorLand/Foundation#ReadMe",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "hello@playform.cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./out/extension",
	"browser": "./dist/browser/extension",
	"scripts": {
		"build-ext": "node ../../node_modules/gulp/bin/gulp.js --gulpfile ../../build/gulpfile.extensions.js compile-extension:markdown-language-features ./tsconfig.json",
		"build-preview": "node ./esbuild-preview",
		"compile": "gulp compile-extension:markdown-language-features && npm run build-preview",
		"compile-web": "npx webpack-cli --config extension-browser.webpack.config --mode none",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"watch-web": "npx webpack-cli --config extension-browser.webpack.config --mode none --watch --info-verbosity verbose"
	},
	"contributes": {
		"commands": [
			{
				"category": "Simple Browser",
				"command": "simpleBrowser.show",
				"title": "Show"
			}
		],
		"configuration": [
			{
				"properties": {
					"simpleBrowser.focusLockIndicator.enabled": {
						"default": true,
						"description": "%configuration.focusLockIndicator.enabled.description%",
						"title": "Focus Lock Indicator Enabled",
						"type": "boolean"
					}
				},
				"title": "Simple Browser"
			}
		]
	},
	"activationEvents": [
		"onCommand:simpleBrowser.api.open",
		"onOpenExternalUri:http",
		"onOpenExternalUri:https",
		"onWebviewPanel:simpleBrowser.view"
	],
	"dependencies": {
		"@vscode/extension-telemetry": "0.9.6"
	},
	"devDependencies": {
		"@playform/build": "0.0.4",
		"@types/vscode-webview": "1.57.5",
		"vscode-codicons": "0.0.14"
	},
	"publishConfig": {
		"access": "public"
	},
	"icon": "media/icon.png",
	"aiKey": "0c6ae279ed8443289764825290e4f9e2-1a736e7c-1324-4338-be46-fc2a58ae4d14-7255",
	"capabilities": {
		"untrustedWorkspaces": {
			"supported": true
		},
		"virtualWorkspaces": true
	},
	"enabledApiProposals": [
		"externalUriOpener"
	],
	"extensionKind": [
		"ui",
		"workspace"
	]
}
