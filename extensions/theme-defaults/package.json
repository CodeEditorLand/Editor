{
	"name": "theme-defaults",
	"displayName": "%displayName%",
	"version": "0.0.1",
	"private": false,
	"description": "%description%",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"scripts": {
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'"
	},
	"contributes": {
		"iconThemes": [
			{
				"id": "vs-minimal",
				"label": "%minimalIconThemeLabel%",
				"path": "./fileicons/vs_minimal-icon-theme.json"
			}
		],
		"themes": [
			{
				"id": "Default Dark+",
				"label": "%darkPlusColorThemeLabel%",
				"path": "./themes/dark_plus.json",
				"uiTheme": "vs-dark"
			},
			{
				"id": "Default Dark Modern",
				"label": "%darkModernThemeLabel%",
				"path": "./themes/dark_modern.json",
				"uiTheme": "vs-dark"
			},
			{
				"id": "Default Light+",
				"label": "%lightPlusColorThemeLabel%",
				"path": "./themes/light_plus.json",
				"uiTheme": "vs"
			},
			{
				"id": "Default Light Modern",
				"label": "%lightModernThemeLabel%",
				"path": "./themes/light_modern.json",
				"uiTheme": "vs"
			},
			{
				"id": "Visual Studio Dark",
				"label": "%darkColorThemeLabel%",
				"path": "./themes/dark_vs.json",
				"uiTheme": "vs-dark"
			},
			{
				"id": "Visual Studio Light",
				"label": "%lightColorThemeLabel%",
				"path": "./themes/light_vs.json",
				"uiTheme": "vs"
			},
			{
				"id": "Default High Contrast",
				"label": "%hcColorThemeLabel%",
				"path": "./themes/hc_black.json",
				"uiTheme": "hc-black"
			},
			{
				"id": "Default High Contrast Light",
				"label": "%lightHcColorThemeLabel%",
				"path": "./themes/hc_light.json",
				"uiTheme": "hc-light"
			}
		]
	},
	"devDependencies": {
		"typescript-esbuild": "0.4.5"
	},
	"publishConfig": {
		"access": "public"
	}
}
