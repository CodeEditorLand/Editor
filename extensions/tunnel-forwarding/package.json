{
	"name": "tunnel-forwarding",
	"displayName": "%displayName%",
	"description": "%description%",
	"main": "./out/extension",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "gulp compile-extension:tunnel-forwarding",
		"prepublishOnly": "Build 'Source/**/*.ts'"
	},
	"contributes": {
		"commands": [
			{
				"category": "%category%",
				"command": "tunnel-forwarding.showLog",
				"enablement": "tunnelForwardingHasLog",
				"title": "%command.showLog%"
			},
			{
				"category": "%category%",
				"command": "tunnel-forwarding.restart",
				"enablement": "tunnelForwardingIsRunning",
				"title": "%command.restart%"
			}
		]
	},
	"activationEvents": [
		"onTunnel"
	],
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@types/node": "20.x"
	},
	"icon": "media/icon.png",
	"capabilities": {
		"untrustedWorkspaces": {
			"supported": true
		},
		"virtualWorkspaces": false
	},
	"enabledApiProposals": [
		"resolvers",
		"tunnelFactory"
	]
}
