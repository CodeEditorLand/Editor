{
  "name": "vscode-colorize-tests",
  "description": "Colorize tests for VS Code",
  "version": "0.0.1",
  "publisher": "playform",
  "license": "MIT",
  "private": "false",
  "activationEvents": [
    "onLanguage:json"
  ],
  "main": "./out/colorizerTestMain",
  "icon": "media/icon.png",
  "scripts": {
    "vscode:prepublish": "node ../../node_modules/gulp/bin/gulp.js --gulpfile ../../build/gulpfile.extensions.js compile-extension:vscode-colorize-tests ./tsconfig.json",
    "prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'"
  },
  "dependencies": {
    "jsonc-parser": "3.2.0"
  },
  "devDependencies": {
    "@types/node": "18.x",
    "typescript-esbuild": "0.3.1"
  },
  "contributes": {
    "semanticTokenTypes": [
      {
        "id": "testToken",
        "description": "A test token"
      }
    ],
    "semanticTokenModifiers": [
      {
        "id": "testModifier",
        "description": "A test modifier"
      }
    ],
    "semanticTokenScopes": [
      {
        "scopes": {
          "testToken": [
            "entity.name.function.special"
          ]
        }
      }
    ],
    "productIconThemes": [
      {
        "id": "Test Product Icons",
        "label": "The Test Product Icon Theme",
        "path": "./producticons/test-product-icon-theme.json",
        "_watch": true
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  }
}
