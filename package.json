    "kill-watch-extensionsd": "deemon --kill yarn watch-extensions",
    "precommit": "node build/hygiene.js",
    "gulp": "node --max_old_space_size=8192 ./node_modules/gulp/bin/gulp.js",
    "electron": "node build/lib/electron",
    "7z": "7z",
