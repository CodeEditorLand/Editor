{
  "name": "vscode-automation",
  "version": "0.0.1",
  "description": "VS Code UI automation driver",
  "author": {
    "name": "Nikola Hristov",
    "email": "nikola@nikolahristov.tech",
    "url": "https://nikolahristov.tech"
  },
  "license": "MIT",
  "main": "./out/index.js",
  "private": "false",
  "scripts": {
    "compile": "npm run copy-driver-definition && node ../../node_modules/typescript/bin/tsc",
    "watch": "npm-run-all -lp watch-driver-definition watch-tsc",
    "watch-tsc": "node ../../node_modules/typescript/bin/tsc --watch --preserveWatchOutput",
    "copy-driver-definition": "node tools/copy-driver-definition.js",
    "watch-driver-definition": "watch \"node tools/copy-driver-definition.js\"",
    "copy-package-version": "node tools/copy-package-version.js",
    "prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'"
  },
  "dependencies": {
    "mkdirp": "1.0.4",
    "ncp": "2.0.0",
    "tmp": "0.2.1",
    "tree-kill": "1.2.2",
    "vscode-uri": "3.0.2"
  },
  "devDependencies": {
    "@types/mkdirp": "1.0.1",
    "@types/ncp": "2.0.1",
    "@types/node": "18.x",
    "@types/tmp": "0.2.2",
    "cpx2": "3.0.0",
    "npm-run-all": "4.1.5",
    "watch": "1.0.2",
    "typescript-esbuild": "0.3.1"
  },
  "homepage": "https://github.com/CodeEditorLand/CodeEditorLand#readme",
  "bugs": {
    "url": "https://github.com/CodeEditorLand/CodeEditorLand/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeEditorLand/CodeEditorLand.git"
  },
  "type": "module",
  "publisher": "playform",
  "publishConfig": {
    "access": "public"
  }
}
